<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="match">
        <attr name="text" format="string" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="recycler_swipe_SwipeMenuLayout">
        <attr name="leftViewId" format="reference"/>
        <attr name="rightViewId" format="reference"/>
        <attr name="contentViewId" format="reference"/>
    </declare-styleable>

    <declare-styleable name="BorderTextView">
        <attr name="borderStrokeWidth" format="float"/>
        <attr name="borderColor" format="color"/>
        <attr name="cornerRadius" format="float"/>
    </declare-styleable>
</resources>
